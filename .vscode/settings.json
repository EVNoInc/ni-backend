{
    "todohighlight.include": [
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss"
    ],
    "todohighlight.keywords": [
        {
            "text": "NOTE:",
            "color": "#f5dd74",
            "backgroundColor": "rgba(0,0,0,.2)",
            "border": "1px solid #f5dd74",
            "borderRadius": "2px",
            "overviewRulerColor": "#f5dd74",
            "isWholeLine": true,
        },
        {
            "text": "INFO:",
            "color": "#3890e2",
            "backgroundColor": "rgba(0,0,0,.2)",
            "border": "1px solid #3890e2",
            "borderRadius": "2px",
            "overviewRulerColor": "#3890e2",
            "isWholeLine": true,
        },
        {
            "text": "TODO:",
            "color": "#76eb99",
            "backgroundColor": "rgba(0,0,0,.2)",
            "border": "1px solid #76eb99",
            "borderRadius": "2px",
            "overviewRulerColor": "#76eb99",
            "isWholeLine": true,
        },
        {
            "text": "HACK:",
            "color": "#ff0000",
            "border": "2px solid #ffffff",
            "borderRadius": "3px",
            "overviewRulerColor": "#000000",
            "backgroundColor": "rgba(0,0,0,.2)",
            "isWholeLine": true,
        },
        {
            "text": "FIXME:",
            "color": "#eb6b8b",
            "border": "2px solid #eb6b8b",
            "borderRadius": "3px",
            "backgroundColor": "rgba(0,0,0,.2)",
            "overviewRulerColor": "#eb6b8b",
            "isWholeLine": true,
        },
        {
            "text": "BUG:",
            "color": "#ff0000",
            "border": "2px solid #ff0000",
            "borderRadius": "3px",
            "backgroundColor": "rgba(0,0,0,1)",
            "overviewRulerColor": "#ff0000",
            "isWholeLine": true,
        },
    ],
    "color-highlight.markerType": "backrgound",
    "color-highlight.enable": false,
    "workbench.colorCustomizations": {
        "statusBar.background": "#f16c6c",
        "statusBar.debuggingBackground": "#f16c6c",
        "statusBar.noFolderBackground": "#f16c6c",
        "statussBar.prominentBackground": "#f16c6c",
    },
    "todo.colors.types": {
        "TODO": "#76eb99",
        "FIXME": "#eb6b8b",
        "FIX": "#eb6b8b",
        "BUG": "#000000",
        "UGLY": "#cc0000",
        "DEBUG": "#cc0000",
        "HACK": "#cc0000",
        "REVIEW": "#00ccff",
        "OPTIMIZE": "#00ccff",
        "TSC": "#00ccff",
        "NOTE": "#f5dd74",
        "IDEA": "#cc00cc",
        "INFO": "#3890e2",
    },
    "todo.timekeeping.created.enabled": true,
    "todo.timekeeping.elapsed.format": "long"
}
